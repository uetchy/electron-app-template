{
  "name": "electron-cra-starter",
  "version": "0.0.0",
  "author": {
    "name": "Your Name",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "main": "public/electron.js",
  "scripts": {
    "build": "craco build",
    "dev": "run-p dev:*",
    "dev:cra": "BROWSER=none craco start",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "eject": "craco eject",
    "package": "build -mw",
    "postinstall": "electron-builder install-app-deps",
    "prepackage": "yarn build",
    "test": "craco test"
  },
  "dependencies": {
    "@craco/craco": "^5.2.1",
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-tippy": "^1.2.3",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^20.43.0",
    "husky": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "typescript": "^3.5.1",
    "wait-on": "^3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": ".",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "unlicense",
  "private": true
}
